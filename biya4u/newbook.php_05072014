<script>
	
			function calbut() {
				var MINIMUM_DECLARED_VALUE = 500;
				var FOR_EVERY_AMOUNT = 100;

				var weight_divisor = $("#selDivisor").val() ? Number($("#selDivisor").val()) : 3500;
				var totalDeclaredValue = 0;

				var lengthVal = Number($("#length").val());
				var widthVal = Number($("#width").val());
				var heightVal = Number($("#height").val());

				if(document.getElementById('length').value) {
					var volumeLWH = lengthVal * widthVal * heightVal;
					$("#dimtot").val(volumeLWH);
					$("#txtDimensionWeight").val( Math.round(volumeLWH/weight_divisor) );
				}

				if(Number($("#dc").val()) > 0) {
					var dcVal = Number($("#dc").val());
					var dcCharge = 0;
					if(dcVal > MINIMUM_DECLARED_VALUE){
						dcVal -= MINIMUM_DECLARED_VALUE; //Subtract 500 from the declared value when its value is greater than 500
						dcCharge = parseInt( dcVal / FOR_EVERY_AMOUNT ); //For each 100 pesos in excess of 500 declared value, you add a Php 1 charge.
					}
					totalDeclaredValue += dcCharge;
				}

				var dimensionWeight = Number($("#txtDimensionWeight").val());
				var actualWeight = Number($("#txtActualWeight").val());
				
				var wei = 0;
				if( dimensionWeight > actualWeight )
					wei = dimensionWeight;
				else
					wei = actualWeight;

				count = Number($("#val").val());
				
				for( i = 1; i <= count; i++ ) {
					//t  = "txtDimensionWeight" + i;
					l  = "length" + i;
					w  = "width"  + i;
					h  = "height" + i;
					dt = "dimtot" + i;
					dc = i + "dc";

					t1 = "txtDimensionWeight" + i;
					t2 = "txtActualWeight" + i;


						

					var lVal = Number($("#" + l).val());
					var wVal = Number($("#" + w).val());
					var hVal = Number($("#" + h).val());
					
					if( lVal ) {
						var vLWH = lVal * wVal * hVal;
						$("#" + dt).val(vLWH);
						$("#" + t1).val( Math.round(vLWH/weight_divisor) );
					}

					var dimW = parseFloat($("#" + t1).val());
					var actW = parseFloat($("#" + t2).val());
					var xWeight = 0;
					
					if( dimW > actW )
						xWeight = dimW;
					else
						xWeight = actW;

					wei += xWeight;
					
					if(Number($("#" + dc).val()) > 0) {
						var dcVal = Number($("#" + dc).val());
						var dcCharge = 0;
						if(dcVal > MINIMUM_DECLARED_VALUE){
							dcVal -= MINIMUM_DECLARED_VALUE; //Subtract 500 from the declared value when its value is greater than 500
							dcCharge = parseInt( dcVal / FOR_EVERY_AMOUNT ); //For each 100 pesos in excess of 500 declared value, you add a Php 1 charge.
						}
						totalDeclaredValue += dcCharge;
					}
				}

				var total_weight = isNaN(wei) ? 0 : wei;
				$("#total_wei").val(total_weight);

				route    = document.getElementById('croute').checked;
				var weii = $('input[name="delarea"]').val();

				/*
				console.log("Method: calbut()");
				console.log("Suggest: [" + wei + "]");
				console.log("Area: [" + weii + "]");
				console.log("CRoute: [" + route + "]");
				console.log("Total Declared Value: [" + totalDeclaredValue + "]");
				*/

				$.get("total_calc.php",{suggest:wei, area:weii, croute:route, totalDeclaredValue: totalDeclaredValue},function(result){
					$("#total").html(result);
				});
			}
	
			function weical(str) {
				var weight_divisor = $("#selDivisor").val() ? Number($("#selDivisor").val()) : 3500;
				var lengthVal = Number($("#length").val());
				var widthVal = Number($("#width").val());
				var heightVal = Number($("#height").val());

				if( lengthVal ) {
					var volumeLWH = lengthVal * widthVal * heightVal;
					$("#dimtot").val(volumeLWH);
					$("#txtDimensionWeight").val( Math.round( volumeLWH / weight_divisor ) );
				}

				wei = parseFloat($("#txtDimensionWeight").val());
				console.log("W1: [" + wei + "]");

				var sud = Number(str) + 1;
				for(i = 1; i < sud; i++) {
					t = "txtDimensionWeight" + i;
					l = "length" + i;
					w = "width" + i;
					h = "height" + i;
					dt = "dimtot" + i;
					
					var lVal = Number($("#" + l).val());
					var wVal = Number($("#" + w).val());
					var hVal = Number($("#" + h).val());
					
					if( lVal ) {
						volumeLWH = lVal * wVal * hVal;
						$("#" + dt).val(volumeLWH);
						$("#" + t).val(Math.round( ( volumeLWH/weight_divisor ) ));
					}
					temp = $("#" + t).val();
					wei  += parseFloat(temp);
					
					console.log("W" + (i+1) + ": [" + wei + "]");
				}

				var total_weight = isNaN(wei) ? 0 : wei;
				$("#total_wei").val(total_weight);

				route = document.getElementById('croute').checked;
				weii  = $('input[name="hiddeninput"]').val();

				/*
				console.log("Method: weighcal()");
				console.log("Suggest: [" + wei + "]");
				console.log("Area: [" + weii + "]");
				console.log("CRoute: [" + route + "]");
				console.log("Divisor: [" + weight_divisor + "]");
				*/

				$.get("total_calc.php",{suggest:wei, area:weii, croute:route}, function(result){
					$("#total").html(result);
				});
			}
	
			function mine(str) {
				var sud = parseInt(str) + 1;	
				var selectString = "";
				var qun = 'qun'+sud;
				var tot = "tot"+sud;
				var pri = 'price'+sud;
