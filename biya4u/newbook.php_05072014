				//Copy options from Type of Shipment Dropdown
				$('#selTypeOfShipment option').each(function(){
					selectString +=  "<option value='" + $(this).val() + "'>" + $(this).text() + "</option>\n";
				});

				var strHtml = "";
				strHtml += "    <tr id=\"" + sud + "\">\n";
				strHtml += "		<td>\n";
				strHtml += "			<select id=\"#selTypeOfShipment" + sud + "\" name=\"tyship[]\" style=\"width:200px;\">\n";
				strHtml += "				" + selectString + "\n";
				strHtml += "			</select>\n";
				strHtml += "		</td>\n";
				strHtml += "		<td align=\"center\"><input type=\"text\" id=\"qun" + sud + "\" name=\"qun[]\" size=\"3\" style=\"text-align: center;\" /></td>\n";
				strHtml += "		<td>\n";
				strHtml += "			<input type=\"text\" name=\"length[]\" id=\"length" + sud + "\" size=\"2\" style=\"text-align:center;\" placeholder=\"L\" onBlur=\"computeDimensionTotal('" + sud + "');\" />\n";
				strHtml += "			<input type=\"text\" name=\"width[]\" id=\"width" + sud + "\" size=\"2\" style=\"text-align:center;\" placeholder=\"W\" onBlur=\"computeDimensionTotal('" + sud + "');\" />\n";
				strHtml += "			<input type=\"text\" name=\"height[]\" id=\"height" + sud + "\" size=\"2\" style=\"text-align:center;\" placeholder=\"H\" onBlur=\"computeDimensionTotal('" + sud + "');\" />\n"; 
				strHtml += "			<input type=\"text\" name=\"dimtot\" id=\"dimtot" + sud + "\" size=\"2\" readonly=\"readonly\" style=\"margin-left: 10px; width: 75px;\" onFocus=\"weical('" + sud + "')\" />\n";
				strHtml += "		</td>\n";
				strHtml += "		<td>\n";
				strHtml += "			<input type=\"text\" name=\"weight[]\" id=\"txtDimensionWeight" + sud + "\" class=\"tot\" value=\"\" style=\"width:50px;text-align: center;\" placeholder=\"Volume\" readonly=\"readonly\" />\n";
				strHtml += "			<input type=\"text\" name=\"weight[]\" id=\"txtActualWeight" + sud + "\" class=\"tot\" value=\"\" style=\"margin-left: 10px; width:50px;text-align: center;\" placeholder=\"Actual\"/>\n";
				strHtml += "		</td>\n";
				strHtml += "		<td>\n";
				strHtml += "			<input type=\"text\" name=\"dc[]\" id=\"" + sud + "dc\" class=\"dc\" value=\"\" style=\"width:100px; text-align: center;\">\n";
				strHtml += "			<span id=\"" + sud + "btnAddItem\" class=\"addHawbItemButton\" onClick=\"mine('" + sud + "'); hideElement('" + sud + "btnAddItem');\">+</span>\n";
				strHtml += "			<span id=\"" + sud + "btnRemoveItem\" class=\"removeHawbItemButton\" onClick=\"removeed('" + sud + "');\">-</span>\n";
				strHtml += "		</td>\n";
				strHtml += "	</tr>\n";

				$("#tblHawbItems").append(strHtml);
				$("#val").val(sud);
			}
	
	function computeDimensionTotal(i) {
		var sud;
		if(!i){
			sud = "0";
			i = "";
		}
		else {
			sud = i;
		}

		var objLength = $("#length" + i);
		var objWidth = $("#width" + i);
		var objHeight = $("#height" + i);

		/*
		console.log("Length: [" + objLength.val() + "]");
		console.log("Width: [" + objWidth.val() + "]");
		console.log("Height: [" + objHeight.val() + "]");
		*/

		if( objLength.val() && objWidth.val() && objHeight.val() ){
			weical(i);
		}
		else if( !objLength.val() && !objWidth.val() && !objHeight.val() ){
			$("#dimtot"+i).val('');
			$("#weight"+i).val('');
			$("#" +i+ "dc").val('');
		}
	}
 
